software =
{
    version = "0.2";
    name = "cip software update"
    images: ({
            filename = "${ROOTFS_PARTITION_NAME}";
            device = "/dev/mmcblk0p2,/dev/mmcblk0p3";
            type = "roundrobin";
            compressed = "zlib";
            filesystem = "ext4";
            properties: {
                        subtype = "image";
            };
    });
}

